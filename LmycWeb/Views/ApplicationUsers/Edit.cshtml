@model LmycWeb.Models.ApplicationUser

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>ApplicationUser</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <input asp-for="FirstName" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="LastName" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="Street" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="City" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="Province" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="PostalCode" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="Country" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="MobileNumber" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="SailingExperience" class="form-control" type="hidden" />
            </div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <input asp-for="UserName" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="NormalizedUserName" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="Email" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="NormalizedEmail" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="EmailConfirmed" type="hidden" /> @Html.DisplayNameFor(model => model.EmailConfirmed)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <input asp-for="PasswordHash" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="SecurityStamp" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="ConcurrencyStamp" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input asp-for="PhoneNumber" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="PhoneNumberConfirmed" type="hidden" /> @Html.DisplayNameFor(model => model.PhoneNumberConfirmed)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="TwoFactorEnabled" type="hidden" /> @Html.DisplayNameFor(model => model.TwoFactorEnabled)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <input asp-for="LockoutEnd" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="LockoutEnabled" type="hidden" /> @Html.DisplayNameFor(model => model.LockoutEnabled)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <input asp-for="AccessFailedCount" class="form-control" type="hidden" />
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
